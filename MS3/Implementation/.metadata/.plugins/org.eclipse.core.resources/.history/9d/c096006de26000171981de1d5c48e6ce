package application.util;

public enum EURI {
	
	SIGNUP ("%ssignup"),
	LOGIN ("%slogin"),
	LOGOUT ("%slogout"),
	DASHBOARD ("%dashboard"),
	PROFILE ("%sprofile"),
	MESSAGES ("%smessages/index"),
	MESSAGES_NEW ("%messages/new"),
	MESSAGE ("%messages/%d"),
	PROJECTS ("%projects/index")
	
	
	
	private final String uri;
	
	EURI(String uri) { this.uri = uri; }
	
	public String uri() { return uri; }
}
