{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
        "include": ["server.js", "util/", "routing/"],
        "exclude": ["./node_modules"],
        "includePattern": ".+\\.js(doc|x)?$"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "template": "templates/default",
        "encoding": "utf8",
        "destination" : "../Dokumentation/Server/",
        "recurse": true
    }
}